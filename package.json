{
  "name": "tablestore-odm",
  "version": "0.0.3",
  "description": "A simple and powerful Object Document Mapper (ODM) for Aliyun Tablestore, built with TypeScript and Zod.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run clean && npm run build",
    "example": "tsc && node dist/examples/basic-usage.js"
  },
  "keywords": [
    "tablestore",
    "ots",
    "aliyun",
    "odm",
    "database",
    "typescript",
    "zod"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OOWhereAreYou/tablestore-odm.git"
  },
  "author": "OOWhereAreYou",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.5.0",
    "tablestore": "^5.6.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "@types/tablestore": "^5.1.3",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist/src/",
    "LICENSE",
    "readme.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
